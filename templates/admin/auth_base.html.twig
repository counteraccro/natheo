<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %} {{ "user.login.title" | trans({}, 'user') }}{% endblock %}</title>
    <link rel="icon"
            href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {%set theme = get_option_system_value_by_key(constant('App\\Utils\\Options\\OptionSystemKey::OS_THEME_SITE')) %}
        {{ encore_entry_link_tags('admin_'~theme) }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('admin') }}
    {% endblock %}
</head>
<body>

    {% block body %}{% endblock %}
</body>
</html>
